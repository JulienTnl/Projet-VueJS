ole.log(error);\n      });\n    },\n\n    scrollTrigger() {\n      const observer = new IntersectionObserver(entries => {\n        entries.forEach(entry => {\n          if (entry.intersectionRatio > 0 && this.nextUrl) {\n            this.next();\n          }\n        });\n      });\n      observer.observe(this.$refs.infinitescrolltrigger);\n    },\n\n    next() {\n      this.currentUrl = this.nextUrl;\n      this.fetchData();\n    },\n\n    setPokemonUrl(url) {\n      this.$emit('setPokemonUrl', url);\n    }\n\n  },\n\n  created() {\n    this.currentUrl = this.apiUrl;\n    this.fetchData();\n  },\n\n  mounted() {\n    this.scrollTrigger();\n  }\n\n};",{"version":3,"sources":["PokemonList.vue"],"names":[],"mappings":";;;;;;;;;;;;;;AAeA,eAAA;AACA,EAAA,KAAA,EAAA,CACA,UADA,EAEA,QAFA,CADA;AAKA,EAAA,IAAA,EAAA,MAAA;AACA,WAAA;AACA,MAAA,QAAA,EAAA,EADA;AAEA,MAAA,OAAA,EAAA,EAFA;AAGA,MAAA,UAAA,EAAA;AAHA,KAAA;AAKA,GAXA;AAYA,EAAA,OAAA,EAAA;AACA,IAAA,SAAA,GAAA;AACA,UAAA,GAAA,GAAA,IAAA,OAAA,CAAA,KAAA,UAAA,CAAA;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,CACA,IADA,CACA,IAAA,IAAA;AACA,YAAA,IAAA,CAAA,MAAA,KAAA,GAAA,EACA,OAAA,IAAA,CAAA,IAAA,EAAA;AACA,OAJA,EAKA,IALA,CAKA,IAAA,IAAA;AACA,aAAA,OAAA,GAAA,IAAA,CAAA,IAAA;AACA,QAAA,IAAA,CAAA,OAAA,CAAA,OAAA,CAAA,OAAA,IAAA;AACA,UAAA,OAAA,CAAA,EAAA,GAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,GAAA,EACA,MADA,CACA,UAAA,IAAA,EAAA;AAAA,mBAAA,CAAA,CAAA,IAAA;AAAA,WADA,EACA,GADA,EAAA;AAEA,eAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,SAJA;AAKA,OAZA,EAaA,KAbA,CAaA,KAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAfA;AAgBA,KAnBA;;AAoBA,IAAA,aAAA,GAAA;AACA,YAAA,QAAA,GAAA,IAAA,oBAAA,CAAA,OAAA,IAAA;AACA,QAAA,OAAA,CAAA,OAAA,CAAA,KAAA,IAAA;AACA,cAAA,KAAA,CAAA,iBAAA,GAAA,CAAA,IAAA,KAAA,OAAA,EAAA;AACA,iBAAA,IAAA;AACA;AACA,SAJA;AAKA,OANA,CAAA;AAQA,MAAA,QAAA,CAAA,OAAA,CAAA,KAAA,KAAA,CAAA,qBAAA;AACA,KA9BA;;AA+BA,IAAA,IAAA,GAAA;AACA,WAAA,UAAA,GAAA,KAAA,OAAA;AACA,WAAA,SAAA;AACA,KAlCA;;AAmCA,IAAA,aAAA,CAAA,GAAA,EAAA;AACA,WAAA,KAAA,CAAA,eAAA,EAAA,GAAA;AACA;;AArCA,GAZA;;AAmDA,EAAA,OAAA,GAAA;AACA,SAAA,UAAA,GAAA,KAAA,MAAA;AACA,SAAA,SAAA;AACA,GAtDA;;AAuDA,EAAA,OAAA,GAAA;AACA,SAAA,aAAA;AACA;;AAzDA,CAAA","sourcesContent":["<template>\r\n  <div class=\"list\">\r\n    <article v-for=\"(pokemon, index) in pokemons\"\r\n    :key=\"'poke'+index\"\r\n    @click=\"setPokemonUrl(pokemon.url)\">\r\n      <img :src=\"imageUrl + pokemon.id + '.png'\" width=\"96\" height=\"96\" alt=\"\">\r\n      <h3>{{ pokemon.name }}</h3>\r\n    </article>\r\n    <div id=\"scroll-trigger\" ref=\"infinitescrolltrigger\">\r\n      <i class=\"fas fa-spinner fa-spin\"></i>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  export default {\r\n    props: [\r\n      'imageUrl',\r\n      'apiUrl'\r\n    ],\r\n    data: () => {\r\n      return {\r\n        pokemons: [],\r\n        nextUrl: '',\r\n        currentUrl: ''\r\n      }\r\n    },\r\n    methods: {\r\n      fetchData() {\r\n        let req = new Request(this.currentUrl);\r\n        fetch(req)\r\n          .then((resp) => {\r\n            if(resp.status === 200)\r\n              return resp.json();\r\n          })\r\n          .then((data) => {\r\n            this.nextUrl = data.next;\r\n            data.results.forEach(pokemon => {\r\n              pokemon.id = pokemon.url.split('/')\r\n                .filter(function(part) { return !!part }).pop();\r\n              this.pokemons.push(pokemon);\r\n            });\r\n          })\r\n          .catch((error) => {\r\n            console.log(error);\r\n          })\r\n      },\r\n      scrollTrigger() {\r\n        const observer = new IntersectionObserver((entries) => {\r\n          entries.forEach(entry => {\r\n            if(entry.intersectionRatio > 0 && this.nextUrl) {\r\n              this.next();\r\n            }\r\n          });\r\n        });\r\n\r\n        observer.observe(this.$refs.infinitescrolltrigger);\r\n      },\r\n      next() {\r\n        this.currentUrl = this.nextUrl;\r\n        this.fetchData();\r\n      },\r\n      setPokemonUrl(url) {\r\n        this.$emit('setPokemonUrl', url);\r\n      }\r\n    },\r\n    created() {\r\n      this.currentUrl = this.apiUrl;\r\n      this.fetchData();\r\n    },\r\n    mounted() {\r\n      this.scrollTrigger();\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n  .list {\r\n    display: grid;\r\n    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));\r\n    grid-gap: 20px;\r\n    width: 100%;\r\n    max-width: 1000px;    \r\n  }\r\n    article {\r\n      height: 200px;\r\n      background-color: #efefef;\r\n      text-align: center;\r\n      text-transform: capitalize;\r\n      border-radius: 10px;\r\n      cursor: pointer;\r\n      padding-top: 15%;\r\n           \r\n    }\r\n    article:hover{\r\n      background-color: rgb(172, 170, 170);\r\n    }\r\n    h3 {\r\n        margin: 0;\r\n      }\r\n  #scroll-trigger {\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    width: 100%;\r\n    height: 150px;\r\n    font-size: 2rem;\r\n    color: #efefef;\r\n  }\r\n</style>\r\n\r\n"],"sourceRoot":"src/components"}]}